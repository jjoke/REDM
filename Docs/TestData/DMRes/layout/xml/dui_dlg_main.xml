	<style id="style-edit" ncskin="ncbg" ncmargin="2,2,2,2" font="face:宋体,size:13,bold:0,underline:0,italic:0,strike:0" />
  </style>
  <skin name="myprivate">
    <!--局部skin对象,外部可在窗口销毁时释放-->
